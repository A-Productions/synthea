        "system": "SNOMED-CT",
        "code": 373066001,
        "display": "Yes"
      }
    },
    "Anxiety_Check": {
      "type": "Simple",
      "conditional_transition": [
        {
          "transition": "Diagnose_Anxiety",
          "condition": {
            "condition_type": "And",
            "conditions": [
              {
                "condition_type": "Attribute",
                "attribute": "anxiety",
                "operator": "==",
                "value": true
              },
              {
                "condition_type": "Not",
                "condition": {
                  "condition_type": "Active Condition",
                  "codes": [
                    {
                      "system": "SNOMED-CT",
                      "code": 48694002,
                      "display": "Anxiety (finding)"
                    }
                  ]
                }
              }
            ]
          }
        },
        {
          "transition": "End_Anxiety",
          "condition": {
            "condition_type": "And",
            "conditions": [
              {
                "condition_type": "Attribute",
                "attribute": "anxiety",
                "operator": "==",
                "value": false
              },
              {
                "condition_type": "Active Condition",
                "codes": [
                  {
                    "system": "SNOMED-CT",
                    "code": 48694002,
                    "display": "Anxiety (finding)"
