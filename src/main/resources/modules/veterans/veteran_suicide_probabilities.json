    "Initial": {
      "type": "Initial",
      "direct_transition": "Veteran check"
    },
    "Terminal": {
      "type": "Terminal"
    },
    "Veteran check": {
      "type": "Simple",
      "conditional_transition": [
        {
          "condition": {
            "condition_type": "Attribute",
            "attribute": "veteran",
            "operator": "is not nil"
          },
          "transition": "Init_Counter"
        },
        {
          "transition": "Terminal"
        }
      ]
    },
    "Baseline": {
      "type": "SetAttribute",
      "attribute": "probability_of_suicide",
      "direct_transition": "Ideation_Baseline",
      "value": 0.003072
    },
    "Homelessness_Check": {
      "type": "Simple",
      "conditional_transition": [
        {
          "transition": "Increment_Counter_Homelessness",
          "condition": {
            "condition_type": "Attribute",
            "attribute": "homeless",
            "operator": "==",
            "value": true
          }
        },
        {
          "transition": "Hypertension_Check"
        }
      ]
    },
    "End_Counter": {
      "type": "Simple",
      "conditional_transition": [
        {
          "transition": "High Risk",
          "condition": {
            "condition_type": "Attribute",
            "attribute": "sucide_risk_factor_count",
            "operator": ">",
            "value": 5
          }
        },
        {
          "transition": "Medium Risk",
          "condition": {
            "condition_type": "Attribute",
            "attribute": "sucide_risk_factor_count",
            "operator": ">",
            "value": 3
          }
        },
        {
          "transition": "Low Risk",
          "condition": {
            "condition_type": "Attribute",
            "attribute": "sucide_risk_factor_count",
            "operator": ">",
            "value": 1
          }
        },
        {
          "transition": "Baseline"
        }
      ]
    },
