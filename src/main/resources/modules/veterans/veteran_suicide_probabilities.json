{
  "name": "Veteran Suicide Probabilities",
  "remarks": [
    ""
  ],
  "states": {
    "Initial": {
      "type": "Initial",
      "direct_transition": "Veteran check"
    },
    "Terminal": {
      "type": "Terminal"
    },
    "Veteran check": {
      "type": "Simple",
      "conditional_transition": [
        {
          "condition": {
            "condition_type": "Attribute",
            "attribute": "veteran",
            "operator": "is not nil"
          },
          "transition": "Init_Counter"
        },
        {
          "transition": "Terminal"
        }
      ]
    },
    "Baseline": {
      "type": "SetAttribute",
      "attribute": "probability_of_suicide",
      "direct_transition": "Terminal",
      "value": 0.003072
    },
    "Homelessness_Check": {
      "type": "Simple",
      "conditional_transition": [
        {
          "transition": "New_State",
          "condition": {
            "condition_type": "Attribute",
            "attribute": "homeless",
            "operator": "==",
            "value": true
          }
        },
        {
          "transition": "New_State_2"
        }
      ]
    },
    "End_Counter": {
      "type": "Simple",
      "conditional_transition": [
        {
          "transition": "High Risk",
          "condition": {
            "condition_type": "Attribute",
            "attribute": "sucide_risk_factor_count",
            "operator": ">",
            "value": 5
          }
        },
        {
          "transition": "Medium Risk",
          "condition": {
            "condition_type": "Attribute",
            "attribute": "sucide_risk_factor_count",
            "operator": ">",
            "value": 3
          }
        },
        {
          "transition": "Low Risk",
          "condition": {
            "condition_type": "Attribute",
            "attribute": "sucide_risk_factor_count",
            "operator": ">",
            "value": 1
          }
        },
        {
          "transition": "Baseline"
        }
      ]
    },
    "Init_Counter": {
      "type": "SetAttribute",
      "attribute": "sucide_risk_factor_count",
      "direct_transition": "Homelessness_Check",
      "value": 0
    },
    "New_State": {
      "type": "Counter",
      "attribute": "sucide_risk_factor_count",
      "action": "increment",
      "direct_transition": "New_State_2"
    },
    "New_State_2": {
      "type": "Simple",
      "direct_transition": "End_Counter"
    },
    "High Risk": {
      "type": "SetAttribute",
      "attribute": "probability_of_suicide",
      "direct_transition": "Terminal",
      "value": 0.006
    },
    "Low Risk": {
      "type": "SetAttribute",
      "attribute": "probability_of_suicide",
      "direct_transition": "Terminal",
      "value": 0.0072
    },
    "Medium Risk": {
      "type": "SetAttribute",
      "attribute": "probability_of_suicide",
      "direct_transition": "Terminal",
      "value": 0.0144
    }
  }
}
